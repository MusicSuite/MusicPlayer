    "schemas": {
      "Body_rename_songs_replace_put": {
        "properties": {
          "old_song": {
            "$ref": "#/components/schemas/Song"
          },
          "new_song": {
            "$ref": "#/components/schemas/Song"
          }
        },
        "type": "object",
        "required": [
          "old_song",
          "new_song"
        ],
        "title": "Body_rename_songs_replace_put"
      },
      "ConsolePlayer": {
        "properties": {
          "volume": {
            "type": "integer",
            "title": "Volume",
            "default": 60
          },
          "current_song": {
            "allOf": [
              {
                "$ref": "#/components/schemas/Song"
              }
            ]
          },
          "state": {
            "allOf": [
              {
                "$ref": "#/components/schemas/PlayerState"
              }
            ],
            "readOnly": true
          },
          "song_position": {
            "type": "number",
            "title": "Song Position",
            "readOnly": true
          }
        },
        "type": "object",
        "required": [
          "state",
          "song_position"
        ],
        "title": "ConsolePlayer"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "PlayerState": {
        "type": "string",
        "enum": [
          "PLAYING",
          "PAUSED",
          "STOPPED"
        ],
        "title": "PlayerState"
      },
      "Song": {
        "properties": {
          "id": {
            "type": "integer",
            "title": "Id"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "duration": {
            "type": "number",
            "title": "Duration"
          }
        },
        "type": "object",
        "required": [
          "id",
          "title",
          "duration"
        ],
        "title": "Song"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
